	<Grid x:Name="LayoutRoot" Background="White" Width="279" Height="492">
		<Button Content="Печать документов" Height="49" Margin="77,57,10,0" Style="{DynamicResource ButtonStyle1}" VerticalAlignment="Top" FontFamily="/Printer.Screens;component/Fonts/#Buxton Sketch" FontSize="18.667">
			<i:Interaction.Triggers>
				<i:EventTrigger EventName="Click">
					<pi:NavigateToScreenAction TargetScreen="PrinterScreens.Document"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<Button Content="Печать фотографий" Height="49" Margin="78,120,10,0" Style="{DynamicResource ButtonStyle2}" VerticalAlignment="Top" FontSize="18.667" FontFamily="/Printer.Screens;component/Fonts/#Buxton Sketch">
