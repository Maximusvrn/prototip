		</Button>
		<Button Content="Button" HorizontalAlignment="Left" Height="64" Margin="11,0,0,154" Style="{DynamicResource ButtonStyle7}" VerticalAlignment="Bottom" Width="64">
			<i:Interaction.Triggers>
				<i:EventTrigger EventName="Click">
					<pi:NavigateToScreenAction TargetScreen="PrinterScreens.Settings"/>
				</i:EventTrigger>
			</i:Interaction.Triggers>
		</Button>
		<TextBlock Height="32" Margin="10,0,85,209" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Bottom" FontSize="18.667"><Run Language="ru-ru" Text="Настройки печати"/></TextBlock>
		<TextBlock Height="26" Margin="10,0,127,122" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Bottom" FontSize="18.667"><Run Language="ru-ru" Text="Принтер:"/></TextBlock>
		<TextBlock Height="24" Margin="92,0,14,63" Style="{DynamicResource BasicTextBlock-Sketch}" Text="Hp Deskjet F4213" VerticalAlignment="Bottom"/>
		<TextBlock Height="31" Margin="11,6,21,0" Style="{DynamicResource BasicTextBlock-Sketch}" VerticalAlignment="Top" FontSize="18.667"><Run Language="ru-ru" Text="Управление принтером"/></TextBlock>
		<Image HorizontalAlignment="Left" Height="64" Margin="13,0,0,48" Source="ic_local_print_shop_64.png" Stretch="Fill" VerticalAlignment="Bottom" Width="64"/>
